<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#03A9F4"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                app:srcCompat="@drawable/ptiumtas"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="52dp"
                android:layout_margin="@dimen/default_padding"/>

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="MOBILE LEARNING"
                android:textAlignment="center"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                tools:layout_editor_absoluteX="145dp"
                tools:layout_editor_absoluteY="131dp"
                android:layout_margin="@dimen/default_padding"/>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Teknologi Informasi dan Komunikasi"
                android:textAlignment="center"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                tools:layout_editor_absoluteX="92dp"
                tools:layout_editor_absoluteY="181dp"
                android:layout_margin="@dimen/default_padding"/>

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#00BCD4"
        android:text="Tujuan Pembelajaran"
        android:layout_marginLeft="@dimen/default_margin"
        android:layout_marginRight="@dimen/default_margin"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
                android:id="@+id/button2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Pertanyaan Pemantik"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="260dp" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Peta Konsep"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="320dp" />

            <Button
                android:id="@+id/button4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Apersepsi"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="380dp" />

            <Button
                android:id="@+id/button5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Kata Kunci"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="440dp" />

            <Button
                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Materi"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="500dp" />

            <Button
                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#00BCD4"
                android:text="Tentang Aplikasi"
                android:layout_marginLeft="@dimen/default_margin"
                android:layout_marginRight="@dimen/default_margin"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="80dp"
                tools:layout_editor_absoluteY="560dp" />
        </LinearLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>